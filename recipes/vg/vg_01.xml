<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation name="Virtual Greenhouse">
	<integrator name="steps" type="UniSim::Steps">
		<parameter name="maxSteps" value="10"/>
	</integrator>

	<model name="calendar" type="UniSim::Calendar">
		<parameter name="latitude" value="55"/>
		<!--
		<parameter name="timeStep" value="1"/>
		<parameter name="timeUnit" value="h"/>
		-->
	</model>

	<model name="env" type="vg::Environment">
		<model name="records" type="UniSim::Records">
			<parameter name="fileName" value="sel_dk2.txt"/>
		</model>
		<!--
		<model name="Tsky" type="UniSim::Fixed" type2="vg:TemperatureSky">
			<parameter name="parameters" value="((method regular)(value 5.))"/>
		</model>
		-->
	</model>
	
	<output type="plot">
		<parameter name="title" value ="Environment"/>
		<trace label="Julian day" ref="calendar[dayOfYear]"/>
		<trace label="Julian day" ref="calendar[dayOfYear]"/>
<!--		<trace label="Tout" ref="env/records[Tout]"/>-->
	</output>

		
</simulation>