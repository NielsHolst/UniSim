<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation version="1.0">

    <integrator name="steps" type="Steps">
        <parameter name="maxSteps" value="100"/>
	</integrator>
	
	<model name="gs" type="UniSim::Range">
		<parameter name="progress" variable="steps[progress]"/>
		<parameter name="min" value="0"/>
		<parameter name="max" value="2"/>
	</model>
	
	<model name="roots" type="xpest::InputTable">
		<parameter name="fileName" value="partition_roots.txt"/>
		<parameter name="xName" value="growthStage"/>
		<parameter name="yName" value="roots"/>
		<parameter name="xValue" ref="gs[value]"/>
	</model>

	<output type="plot">
		<parameter name="title" value="Partioning coefficients"/>
		<trace label="Growth stage" ref="gs[value]"/>		
		<trace label="Roots" ref="roots[yValue]"/>
	</output>
	
</simulation>


