<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation version="1.0">
	<integrator type="UniSim::TimeStepLimited">
		<parameter name="maxSteps" value="30"/>
		<sequence>
			<model name="calendar"/>
			<model name="withParameterFile"/>
		</sequence>
	</integrator>

	<model name="calendar" type="UniSim::Calendar"/>

	<model name="withParameterFile" type="Test::WithParameterFile">
		<parameter name="crops" fileName="model_with_parameter_file_input.txt"/>
	</model>	
	
	<output type="table">
		<parameter name="fileName" value="test.txt"/>
		<variable label="Day" axis="x" value="calendar[dayOfYear]"/>
		<variable label="Day" axis="y" value="calendar[dayOfYear]"/>
	</output>
</simulation>
