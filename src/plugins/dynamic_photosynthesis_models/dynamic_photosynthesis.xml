<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation version="1.0">
	<integrator type="TimeStepLimited">
		<parameter name="maxSteps" value="60"/>
		<sequence>
			<model name="calendar"/>
			<model name="sunLeaf"/>
			<model name="shadeLeaf"/>
			<model name="weather"/>
		</sequence>
	</integrator>

	<model name="calendar" type="Calendar">
		<parameter name="initialDate" value="1/1/2010"/>
	</model>

	<model name="sunLeaf" type="dynamic_photosynthesis::Leaf">
		<parameter name="Ninit" value="1.02136"/>
		<parameter name="r" value="1.02122"/>
		<parameter name="K" value="113.627"/>
	</model>

	<model name="shadeLeaf" type="dynamic_photosynthesis::Leaf">
		<parameter name="Ninit" value="4.66906"/>
		<parameter name="r" value="1.06908"/>
		<parameter name="K" value="101.444"/>
	</model>

	<model name="weather" type="dynamic_photosynthesis::Weather">
		<parameter name="Ninit" value="1.97855"/>
		<parameter name="r" value="1.04333"/>
		<parameter name="K" value="108.597"/>
	</model>

	<output type="plot">
		<parameter name="title" value ="SunLeaf"/>
		<variable axis="x" label="Day" value="calendar[dayOfYear]"/>
		<variable axis="y" label="Density" value="sunLeaf[N]"/>
	</output>
	<output type="plot">
		<parameter name="title" value ="ShadeLeaf"/>
		<variable axis="x" label="Day" value="calendar[dayOfYear]"/>
		<variable axis="y" label="Density" value="shadeLeaf[N]"/>
	</output>
	<output type="plot">
		<parameter name="title" value ="Weather"/>
		<variable axis="x" label="Day" value="calendar[dayOfYear]"/>
		<variable axis="y" label="Density" value="weather[N]"/>
	</output>
</simulation>
