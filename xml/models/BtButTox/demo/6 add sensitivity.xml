<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation name="simulation" version="1.0">
	<integrator type="TimeStepLimited">
		<parameter name="maxSteps" value="365"/> <!-- all years = 20440 -->
		<sequence>
			<model name="random"/>
			<model name="calendar"/>
			<model name="weather"/>
			<model name="pollen"/>
			<model name="insect"/>
		</sequence>
		<model name="RunIterator" type="UniSim::RunIteratorFixed">
			<parameter name="numIterations" value="5"/>
		</model>
	</integrator>

	<model name="random">
		<model name="dateShift" type="UniSim::RandomUniform">
			<parameter name="minValue" value="-15"/>
			<parameter name="maxValue" value="15"/>
		</model>
		<model name="Pmax" type="UniSim::RandomUniform">
			<parameter name="minValue" value="120"/>
			<parameter name="maxValue" value="240"/>
		</model>
		<model name="timeCompression" type="UniSim::RandomUniform">
			<parameter name="minValue" value="0.5"/>
			<parameter name="maxValue" value="2"/>
		</model>
	</model>

	
	<model name="calendar" type="Calendar"/>

	<model name="weather" type="UniSim::Weather">
		<model name="records" type="UniSim::Records">
			<parameter name="fileName" value="denmark-odum-1953-2009.txt"/>
		</model>
	</model>

	<model name="pollen" type="EcoTox::Pollen">
		<parameter name="halfLife" value="30"/>
		<parameter name="distance" value="1"/>
		<parameter name="dateMax" value="31/7/2011"/>
		<parameter name="dateShift" variable="random/dateShift[value]"/>
		<parameter name="Pmax" variable="random/Pmax[value]"/>
		<parameter name="c" variable="random/timeCompression[value]"/>
	</model>
	
	<model name="insect" type="UniSim::Insect">
		<parameter name="sexRatio" value="0.5"/>
		<parameter name="eggProduction" value="2"/>

		<model name="hibernatingAdult" type="UniSim::Stage">
			<model name="time" type="UniSim::Days">
				<model name="trigger" type="UniSim::TriggerByDate">
					<parameter name="fromDate" value="25/4/1900"/>
					<parameter name="toDate" value="30/5/2200"/>
					<parameter name="frequency" value="yearly"/>
					<parameter name="triggerAtReset" value="No"/>
				</model>
			</model>
			<parameter name="duration" value="7"/>
			<parameter name="k" value="30"/>
			<parameter name="initialInflow" value="1"/>
		</model>

		<model name="generation" type="UniSim::InsectLifeCycle" instances="1">
			<model name ="eggLayingDelay" type="UniSim::Stage" >
				<model name="time" type="UniSim::Days"/>
				<parameter name="duration" value="7"/>
				<parameter name="k" value="30"/>
			</model>
			
			<model name="egg" type="UniSim::Stage">
				<model name="time" type="UniSim::DayDegrees">
					<parameter name="T0" value="8.3" ref="Bryant et al. 2002"/>
					<parameter name="Topt" value="32.3" ref="Bryant et al. 2000"/>
					<parameter name="Tmax" value="37"/>
				</model>
				<parameter name="duration" value="70"/>
				<parameter name="k" value="30"/>
			</model>

			<model name="larva" type="UniSim::Stage">
				<model name="survival" type="EcoTox::LogLogistic">
					<parameter name="upper" value="1"/>
					<parameter name="lower" value="0"/>
					<parameter name="slope" value="-1"/>
					<parameter name="ED50" value="4000"/>
				</model>
				<model name="time" type="UniSim::DayDegrees">
					<parameter name="T0" value="8.3" ref="Bryant et al. 2002"/>
					<parameter name="Topt" value="32.3" ref="Bryant et al. 2000"/>
					<parameter name="Tmax" value="37"/>
				</model>
				<parameter name="duration" value="315.2" ref="Bryant et al. 2002"/>
				<parameter name="k" value="30"/>
			</model>

			<model name="pupa" type="UniSim::Stage">
				<model name="time" type="UniSim::DayDegrees">
					<parameter name="T0" value="10.9" ref="Bryant et al. 2002"/>
					<parameter name="Topt" value="32.3" ref="Bryant et al. 2000"/>
					<parameter name="Tmax" value="37"/>
				</model>
				<parameter name="duration" value="110" ref="Bryant et al. 2002"/>
				<parameter name="k" value="30"/>
			</model>

			<model name="adult" type="UniSim::Stage">
				<model name="time" type="UniSim::Days"/>
				<parameter name="duration" value="60"/>
				<parameter name="k" value="30"/>
			</model>
		</model> <!-- lifeCycle -->
	</model> <!-- insect -->

	<!-- 
	*** Plots  ***
	-->

	<output type="plot">
		<parameter name="title" value="Environment"/>
		<variable label="Day" axis="x" value="calendar[totalTime]"/>
		<variable label="Tavg" axis="y" value="weather[Tavg]"/>
	</output>

	<output type="plot">
		<parameter name="title" value ="One generation"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="Egg" value="generation/egg[value]"/>
		<variable axis="y" label="Larva" value="generation/larva[value]"/>
		<variable axis="y" label="Pupa" value="generation/pupa[value]"/>
		<variable axis="y" label="AdRep" value="generation/adult[value]"/>
	</output>

	<output type="plot">
		<parameter name="title" value ="Population response"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="Larva" value="generation/larva[value]"/>
		<variable axis="y" label="AdHib" value="hibernatingAdult[value]"/>
	</output>
	
	<output type="plot">
		<parameter name="title" value ="Pollen deposition"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="Deposition rate" value="pollen[pollenDepositionRate]"/>
	</output>

	<output type="plot">
		<parameter name="title" value ="Pollen density (per cm2)"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="Deposition rate" value="pollen[pollenDepositionRate]"/>
		<variable axis="y" label="Deposition total" value="pollen[pollenDepositionTotal]"/>
		<variable axis="y" label="Pollen (per cm2)" value="pollen[pollen]"/>
	</output>

	<output type="plot">
		<parameter name="title" value ="Bt exposure (log10)"/>
		<parameter name="logy" value ="yes"/>
		<parameter name="ymin" value ="-2"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="Larva" value="generation/larva[value]"/>
		<variable axis="y" label="Pollen (per cm2)" value="pollen[pollen]"/>
	</output>


	<output type="plot" summary="yes">
		<parameter name="title" value="Net reproduction"/>
		<variable label="Time shift" axis="x" value="random/dateShift[value]"/>		
		<variable axis="y" label="Exposure" value="hibernatingAdult[value]" summary="final"/>
	</output>

	<output type="plot" summary="yes">
		<parameter name="title" value="Net reproduction"/>
		<variable label="Total pollen" axis="x" value="random/Pmax[value]"/>		
		<variable axis="y" label="Exposure" value="hibernatingAdult[value]" summary="final"/>
	</output>

	<output type="plot" summary="yes">
		<parameter name="title" value="Net reproduction"/>
		<variable label="Time compression" axis="x" value="random/timeCompression[value]"/>		
		<variable axis="y" label="Exposure" value="hibernatingAdult[value]" summary="final"/>
	</output>
	
</simulation>
