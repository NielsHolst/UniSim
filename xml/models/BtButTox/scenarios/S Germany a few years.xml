<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation select="doc('S Germany many years.xml')/simulation">
	<integrator type="Steps">
		<parameter name="maxSteps" value="365"/> 
		<model name="RunIterator" type="UniSim::RunIteratorFixed">
			<parameter name="numIterations" value="10"/>
		</model>
	</integrator>

	<!-- 
	*** Plots  ***
	-->
	<output type="plot">
		<parameter name="title" value="Environment"/>
		<variable label="Day" axis="x" value="calendar[totalTime]"/>
		<variable label="Tavg" axis="y" value="weather[Tavg]"/>
	</output>

	<output type="plot">
		<parameter name="title" value ="Pollen"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="Rate" value="pollen[pollenDepositionRate]"/>
		<variable axis="y" label="Density" value="pollen[pollenDensity]"/>
		<variable axis="y" label="Production" value="pollen[pollenDepositionTotal]"/>
	</output>

	<output type="plot">
		<parameter name="title" value ="Inachis io 1. gen"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="Egg" value="generation(1)/egg[value]"/>
		<variable axis="y" label="Larva" value="generation(1)/larva[value]"/>
		<variable axis="y" label="Pupa" value="generation(1)/pupa[value]"/>
		<variable axis="y" label="AdRep" value="generation(1)/adult[value]"/>
	</output>

	<output type="plot">
		<parameter name="title" value ="Inachis io 2. gen"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="Egg" value="generation(2)/egg[value]"/>
		<variable axis="y" label="Larva" value="generation(2)/larva[value]"/>
		<variable axis="y" label="Pupa" value="generation(2)/pupa[value]"/>
		<variable axis="y" label="AdRep" value="generation(2)/adult[value]"/>
	</output>

	<output type="plot">
		<parameter name="title" value ="Population response"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="AdHib" value="hibernatingAdult[value]"/>
	</output>
	
	<output type="plot">
		<parameter name="title" value ="Bt exposure (log10)"/>
		<parameter name="logy" value ="yes"/>
		<parameter name="ymin" value ="-2"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="Larva 1.gen" value="generation(1)/larva[value]"/>
		<variable axis="y" label="Larva 2.gen" value="generation(2)/larva[value]"/>
		<variable axis="y" label="Pollen density" value="pollen[pollenDensity]"/>
	</output>
	
</simulation>
