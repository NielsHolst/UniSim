<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation select="doc('S Germany many years.xml')/simulation">

	<!-- 
	*** File output ***
	-->
	
	<output type="table" name="details">
		<parameter name="filename" value ="sgermany.txt"/>
		<variable axis="x" label="Day" value="calendar[totalTime]"/>
		<variable axis="y" label="Pollen" value="pollen[pollenDensity]"/>
		<variable axis="y" label="Larva1" value="generation(1)/larva[value]"/>
		<variable axis="y" label="Adult1" value="generation(1)/adult[value]"/>
		<variable axis="y" label="AdultHib" value="io/hibernatingAdult[value]"/>
		<variable axis="y" label="Larva2" value="generation(2)/larva[value]"/>
		<variable axis="y" label="Adult2" value="generation(2)/adult[value]"/>
	</output>
	
</simulation>
