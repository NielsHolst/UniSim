	<!-- 
	*** Plots  ***
	-->

	<output type="plot">
		<parameter name="title" value="Temperature"/>
		<variable label="Day" axis="x" value="calendar[dayOfYear]"/>
		<variable label="Tavg" axis="y" value="weather[Tavg]"/>
		<variable label="Day length" axis="y" value="calendar[daylength]"/>
	</output>

	<output type="plot">
		<parameter name="title" value ="Life stage numbers"/>
		<variable axis="x" label="Day" value="calendar[dayOfYear]"/>
		<variable axis="y" label="Density" value="insect/*/lifeTable/number[value]"/>
	</output>
	
	<output type="plot">
		<parameter name="title" value ="Life stage mass"/>
		<variable axis="x" label="Day" value="calendar[dayOfYear]"/>
		<variable axis="y" label="Density" value="insect/*/lifeTable/mass[value]"/>
	</output>

	<output type="plot">
		<parameter name="title" value ="Life stage individual mass"/>
		<variable axis="x" label="Day" value="calendar[dayOfYear]"/>
		<variable axis="y" label="Density" value="insect/*/lifeTable[individualMass]"/>
	</output>

	<!-- 
	*** Tables  ***
	-->
	<output type="table">
		<parameter name="filename" value ="io number.txt"/>
		<variable axis="x" label="Day" value="calendar[dayOfYear]"/>
		<variable axis="y" label="Density" value="insect/*/lifeTable/number[value]"/>
	</output>
	<output type="table">
		<parameter name="filename" value ="io ind mass.txt"/>
		<variable axis="x" label="Day" value="calendar[dayOfYear]"/>
		<variable axis="y" label="Density" value="insect/*/lifeTable[individualMass]"/>
	</output>

</simulation>
