<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation version="1.0">
	<integrator type="UniSim::TimeStepLimited">
		<parameter name="maxSteps" value="90"/>
		<sequence>
			<model name="random"/>
			<model name="calendar"/>
			<model name="weather"/>
			<model name="pollen"/>
			<model name="insect"/>
		</sequence>
		<model name="RunIterator" type="UniSim::RunIteratorFixed">
			<parameter name="numIterations" value="30"/>
		</model>
	</integrator>

	
	<model name="random">
		<model name="fecundity_scale" type="UniSim::RandomUniform">
			<parameter name="minValue" value="0.01"/>
			<parameter name="maxValue" value="1"/>
		</model>
	</model>
	
	
	<model name="calendar" type="UniSim::Calendar">
		<parameter name="followers" value="weather"/>
	</model>

	<model name="weather" type="beehive::Weather">
		<parameter name="fileName" value="AZ_96.txt"/>
		<parameter name="firstDate" value="1/5/1996"/>
	</model>

	<model name="pollen" type="Beehive::Pollen">
		<parameter name="r" value="0.1"/>
	</model>

	<model name="insect" type="Beehive::Insect8">
		<parameter name="initEggs" value="100"/>
		<model name="egg" type="UniSim::Stage">
			<parameter name="duration" value="20"/>
			<parameter name="k" value="30"/>
			<model name="time" type="UniSim::DayDegrees">
				<parameter name="T0" value="12.5"/>
				<parameter name="Topt" value="40"/>
				<parameter name="Tmax" value="40"/>
			</model>
			<model name="survival" type="beehive::EggSurvival">
				<parameter name="a" value="-0.00706"/>
				<parameter name="b" value="0.370"/>
				<parameter name="c" value="-4.12"/>
				<parameter name="Tlow" value="21"/>
			</model>
		</model>
		<model name="larva" type="UniSim::Stage">
			<parameter name="duration" value="21"/>
			<parameter name="k" value="30"/>
			<model name="time" type="UniSim::Days"/>
			<model name="survival" type="beehive::LarvaSurvival">
				<parameter name="demandRate" value="0.5"/>
				<parameter name="attackRate" value="0.2"/>
			</model>
		</model>
		<model name="pupa" type="UniSim::Stage">
			<parameter name="duration" value="7"/>
			<parameter name="k" value="30"/>
			<model name="time" type="UniSim::Days"/>
		</model>
		<model name="adult" type="UniSim::Stage">
			<model name="fecundity" type="Beehive::Fecundity">
				<parameter name="root1" value="0"/>				
				<parameter name="root2" value="10"/>				
				<parameter name="scale" variable="random/fecundity_scale[value]" old_value="0.2"/>				
				<parameter name="sexRatio" value="0.5"/>				
			</model>
			<parameter name="duration" value="28"/>
			<parameter name="k" value="30"/>
			<model name="time" type="UniSim::Days"/>
		</model>
	</model>

	<!-- *** Outputs *** -->
	
	<output type="table">
		<parameter name="filename" value="insect_stages.txt"/>
		<variable label="Day" axis="x" value="calendar[dayInYear]"/>
		<variable label="Egg" axis="y" value="insect/egg[number]"/>
		<variable label="Larva" axis="y" value="insect/larva[number]"/>
		<variable label="Pupa" axis="y" value="insect/pupa[number]"/>
		<variable label="Adult" axis="y" value="insect/adult[number]"/>
		<variable label="Pollen" axis="y" value="pollen[mass]"/>
		<variable label="Pollen120" axis="y" value="pollen[mass]" summary="(xAtThreshold 120.)"/>
		<variable label="Pollen80" axis="y" value="pollen[mass]" summary="(xAtThreshold 80.)"/>
		<variable label="Pollen10" axis="y" value="pollen[mass]" summary="(xAtThreshold 10.)"/>
	</output>

	<output type="plot">
		<parameter name="title" value="Fecundity scaling"/>
		<variable label="Day" axis="x" value="calendar[dayInYear]"/>
		<read-parameter label="scale" axis="y" value="fecundity[scale]"/>
	</output>
	
	<output type="plot">
		<parameter name="title" value="Insect stages"/>
		<variable label="Day" axis="x" value="calendar[dayInYear]"/>
		<variable label="Egg" axis="y" value="insect/egg[number]"/>
		<variable label="Larva" axis="y" value="insect/larva[number]"/>
		<variable label="Pupa" axis="y" value="insect/pupa[number]"/>
		<variable label="Adult" axis="y" value="insect/adult[number]"/>
	</output>
	<output type="plot">
		<parameter name="title" value="Number of eggs laid"/>
		<variable label="Day" axis="x" value="calendar[dayInYear]"/>
		<variable label="Egg" axis="y" value="fecundity[eggsLaid]"/>
	</output>
	<output type="plot">
		<parameter name="title" value="Egg survival"/>
		<variable label="Day" axis="x" value="calendar[dayInYear]"/>
		<variable label="Egg survival" axis="y" value="egg/survival[survival]"/>
	</output>
	<output type="plot">
		<parameter name="title" value="Larva survival"/>
		<variable label="Day" axis="x" value="calendar[dayInYear]"/>
		<variable label="Larva survival" axis="y" value="larva/survival[survival]"/>
	</output>
	<output type="plot">
		<parameter name="title" value="Pollen"/>
		<variable label="Day" axis="x" value="calendar[dayInYear]"/>
		<variable label="Pollen" axis="y" value="pollen[mass]"/>
	</output>
	<output type="plot">
		<parameter name="title" value="Temperature"/>
		<variable label="Day" axis="x" value="calendar[dayInYear]"/>
		<variable label="Tmax" axis="y" value="weather[Tmax]"/>
		<variable label="Tavg" axis="y" value="weather[Tavg]"/>
		<variable label="Tmin" axis="y" value="weather[Tmin]"/>
		<variable label="TavgMin" axis="y" value="weather[Tavg]" summary="min"/>
		<variable label="TavgMax" axis="y" value="weather[Tavg]" summary="max"/>
		<variable label="TavgAvg" axis="y" value="weather[Tavg]" summary="avg"/>
		<variable label="TavgXAtMin" axis="y" value="weather[Tavg]" summary="xAtMin"/>
		<variable label="TavgXAtMax" axis="y" value="weather[Tavg]" summary="xAtMax"/>
	</output>
	<output type="plot" summary="yes">
		<parameter name="title" value="Max. larva density"/>
		<variable label="FecScale" axis="x" value="random/fecundity_scale[value]"/>		
		<variable label="Larva" axis="y" value="insect/larva[number]" summary="max"/>
	</output>
	<output type="plot" summary="yes">
		<parameter name="title" value="Days until pollen = 10"/>
		<variable label="FecScale" axis="x" value="random/fecundity_scale[value]"/>		
		<variable label="Pollen threshold" axis="y" value="pollen[mass]" summary="(xAtThreshold 10)"/>
	</output>
</simulation>
