<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation version="1.0">
	<integrator type="TimeStepLimited">
		<parameter name="maxSteps" value="3"/>
		<sequence>
			<model name="calendar"/>
			<model name="landscape"/>
		</sequence>
	</integrator>
	
	<model name="calendar" type="Calendar">
	  <parameter name="initialDate" value="1/1/2005"/>
	  <parameter name="timeUnit" value="y"/>
	 </model>
	 
	<model name="landscape" type="Landscape">
	    <parameter name="Method_fert" value="YES"/>
	    <parameter name="CropParameters3" fileName="crop_parameters_all_farms_business.txt"/>
		<parameter name="CropAreas" fileName="crop_areas_all.txt"/>
		<parameter name="FarmsData" fileName="farms_data_all.txt"/>	
		<parameter name="LivestockParameters" fileName="livestock_parameters.txt"/>
		<parameter name="LivestockNumbers" fileName="livestock_numbers.txt"/>
		
		<parameter name="WinterMaxValues" fileName="winterMax.txt"/>		
		<parameter name="PriceNt" value="1.93"/>
	</model>
	
