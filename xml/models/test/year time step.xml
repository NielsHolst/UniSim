<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation version="1.0">
	<integrator type="TimeStepLimited">
		<parameter name="maxSteps" value="10"/>
		<sequence>
			<model name="calendar"/>
		</sequence>
	</integrator>
	<model name="calendar" type="Calendar">
		<parameter name="initialDate" value="1/1/2006"/>
		<parameter name="timeUnit" value="y"/>
	</model>
	
	<output type="plot">
		<parameter name="title" value="Test output"/>
		<variable label="Year" axis="x" value="calendar[year]"/>
		<variable label="steps" axis="y" value="calendar[totalTime]"/>
	</output>
</simulation>
