<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation version="1.0">
	<integrator type="UniSim::TimeStepLimited">
		<parameter name="maxSteps" value="365"/>
	</integrator>

	<model name="calendar" type="UniSim::Calendar">
		<parameter name="latitude" value="60.38" comment="Bergen, Norway"/>
	</model>

	<output type="plot">
		<parameter name="title" value="Insect stages"/>
		<variable label="Day" axis="x" value="calendar[dayOfYear]"/>
		<variable label="DayLength" axis="x" value="calendar[dayLength]"/>
	</output>
</simulation>
