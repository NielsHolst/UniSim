<?xml version="1.0" encoding="ISO-8859-1"?>
<simulation version="1.0">
	<integrator type="UniSim::Steps">
		<parameter name="maxSteps" value="100"/>
	</integrator>

	<model name="temperature" type="UniSim::Range">
		<parameter name="min" value="0"/>
		<parameter name="max" value="50"/>
	</model>

	<model name="response" type="EcoTox::LogLogistic">
		<parameter name="dose" variable="temperature[value]"/>
		<parameter name="lower" value="0"/>
		<parameter name="upper" value="100"/>
		<parameter name="ED50" value="38"/>
		<parameter name="slope" value="3.5"/>
	</model>

	<output type="plot">
		<parameter name="title" value="Temperature response"/>
		<trace label="Dose" value="response[dose]"/>
		<trace label="Mortality" value="response[value]"/>
	</output>
</simulation>
